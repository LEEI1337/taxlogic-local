    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
