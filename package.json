{
  "name": "taxlogic-local",
  "productName": "TaxLogic",
  "version": "1.0.0-alpha",
  "description": "AI-powered local tax filing assistant for Austria - 100% privacy-first",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "build": "electron-forge make",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "postinstall": "electron-rebuild"
  },
  "keywords": [
    "tax",
    "austria",
    "steuer",
    "finanzamt",
    "electron",
    "ai",
    "local",
    "privacy",
    "ollama",
    "langgraph"
  ],
  "author": "TaxLogic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taxlogic/taxlogic-local"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.31.0",
    "@electron/remote": "^2.1.2",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@qdrant/js-client-rest": "^1.8.0",
    "axios": "^1.6.7",
    "dotenv": "^17.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "langchain": "^0.3.0",
    "ollama": "^0.5.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.0",
    "sharp": "^0.33.2",
    "sql.js": "^1.10.0",
    "tesseract.js": "^5.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
